<?xml version='1.0' encoding='utf-8'?>
<widget id="com.onesignal.example" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    
    <name>OneSignalExample</name>
    <description>
        An example Cordova app with the OneSignal plugin intergated showing how to use each of it's functions.
    </description>
    <author email="support@onesignal.com" href="https://onesignal.com">
        OneSignal Team
    </author>
    
    <content src="index.html" />
    
    <plugin name="cordova-plugin-whitelist" spec="1" />
    
    <plugin name="onesignal-cordova-plugin" spec="^2.4.5" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    
</widget>
